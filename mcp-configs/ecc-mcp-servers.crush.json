    "args": [
      "-y",
      "@context7/mcp-server"
    ],
    "timeout": 120,
    "disabled": false,
    "description": "Live documentation lookup"
  },
  "magic": {
    "type": "stdio",
    "command": "npx",
    "args": [
      "-y",
      "@magicuidesign/mcp@latest"
    ],
    "timeout": 120,
    "disabled": false,
    "description": "Magic UI components"
